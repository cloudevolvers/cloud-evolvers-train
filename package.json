    "scripts": {
        "dev": "vite --port 4000",
        "dev:fast": "npm run kill:5000 && vite --port 5000 --host --open",
        "dev:vite": "npm run kill:5000 && vite --mode development --port 5000 --host",
        "dev:swa": "npx swa start http://localhost:5000 --api-location api --port 4286 --api-port 7076 --verbose false",
        "dev:swa:wait": "sleep 3 && npm run dev:swa",
        "dev:status": "sleep 10 && echo \"\\nüöÄ Development Environment Ready!\\nüìç Your React App: http://localhost:4286\\nüîß Vite Dev Server: http://localhost:5000 (with MCP at /__mcp/sse)\\n‚ö° Azure Functions: http://localhost:7076/api\\nüí° Use http://localhost:4286 for full app with API integration\\n\"",
        "dev:staging": "vite --port 5174 --mode staging",
        "dev:clean": "vite --port 3002 --host --logLevel warn --clearScreen false",
        "dev:quick": "vite --port 3003 --host --logLevel error --clearScreen false",
        "kill": "./scripts/kill-dev-servers.sh",
        "kill:5000": "./scripts/kill-dev-servers.sh",
        "kill:4286": "./scripts/kill-dev-servers.sh",
        "kill:7076": "./scripts/kill-dev-servers.sh",
        "kill:ports": "./scripts/kill-dev-servers.sh",
        "build": "tsc -b --noCheck && vite build --mode production",
        "build:staging": "tsc -b --noCheck && vite build --mode staging",
        "build:dev": "tsc -b --noCheck && vite build --mode development",
        "lint": "eslint .",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "swa:start": "npx swa start",
        "swa:build": "npx swa build",
        "swa:deploy": "npx swa deploy",
        "swa:deploy:staging": "npx swa deploy --env staging",
        "swa:start:staging": "npx swa start --config cloud-evolvers-train-staging",
        "images": "./image-server/start-image-server.sh start",
        "images:stop": "./image-server/start-image-server.sh stop",
        "images:restart": "./image-server/start-image-server.sh restart",
        "images:status": "./image-server/start-image-server.sh status",
        "images:logs": "./image-server/start-image-server.sh logs",
        "images:download": "./image-server/download-professional-images.sh",
        "images:preview": "./image-server/create-image-preview.sh",
        "images:install": "cd image-server && npm install",
